{% extends "layout.njk" %}
{% block  content %}
<main>
    <h1>{{ title }}</h1>
    <p>{{ intro }}</p>
    <p>Detta är dock statisk text. Följt av data från databas.</p>
    <ul>
        {% for row in rows %}
        <div class="d-flex">
            <li>
                <br> Name: {{row.name}}   <br>   secret_identity: {{row.secret_identity}} <br>
                {{row.bio}} <br>
            </li>
        </div>
        {% endfor %}
    </ul>
</main>
{% endblock %}%